    <div class="site-section">
        <div class="container">
            <div class="row">

                <div class="col-md-8">

                    <div class="row mb-3 align-items-stretch">

                    </div>

                    <div class="col-12 text-center mt-5">
                        <p class="custom-pagination">
                            <span>1</span>
                            <a href="#">2</a>
                            <a href="#">3</a>
                            <span>...</span>
                            <a href="#">20</a>
                        </p>
                    </div>
                </div>

                <div class="col-md-3 ml-auto">
                    <div class="mb-5">
